citHeader("To cite FARMS in publications use:")

citEntry(entry="Article",
         title = "A new summarization method for affymetrix probe level data",
         author = personList(as.person("Sepp Hochreiter"),
	                     as.person("Djork-Arne Clevert"), as.person("Klaus Obermayer")),
         journal      = "Bioinformatics",
         year         = "2006",
	 volume       = "22",
	 number       = "8",
	 pages        = "943--949",
         url          = "http://bioinformatics.oxfordjournals.org/cgi/content/abstract/22/8/943",
         
         textVersion = 
    	paste("Hochreiter, Sepp and Clevert, Djork-Arne and Obermayer, Klaus (2006).", 
               "A new summarization method for affymetrix probe level data",
               "Bioinformatics, Volume 22, Number 8, Pp. 943-949",
	       "http://bioinformatics.oxfordjournals.org/cgi/content/abstract/22/8/943")
	       
)

